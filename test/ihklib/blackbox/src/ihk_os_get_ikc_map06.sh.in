#!/usr/bin/sh

# define WORKDIR
if [ "${AUTOTEST_HOME}" != "" ]; then
    . ${AUTOTEST_HOME}/bin/config.sh
fi

# insmod as root
sudo @CMAKE_INSTALL_PREFIX@/bin/ihk_os_get_ikc_map06 -u $(id -u) -g $(id -g) -i || exit $?
num_cpus=$?
sudo chmod 666 /dev/mcos0

# try to reserve memory as non-root
@CMAKE_INSTALL_PREFIX@/bin/ihk_os_get_ikc_map06 -u $(id -u) -g $(id -g) -n ${num_cpus} || exit $?

# rmmod as root
sudo @CMAKE_INSTALL_PREFIX@/bin/ihk_os_get_ikc_map06 -u $(id -u) -g $(id -g) -r || exit $?

exit 0
